<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SpinnerTheme" parent="android:Theme.Holo.Dialog">
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:windowBackground">@color/m_transparent</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@color/m_transparent</item>
        <item name="android:bottomDark">@color/m_transparent</item>
        <item name="android:bottomMedium">@color/m_transparent</item>
        <item name="android:centerBright">@color/m_transparent</item>
        <item name="android:centerDark">@color/m_transparent</item>
        <item name="android:centerMedium">@color/m_transparent</item>
        <item name="android:fullBright">@color/m_transparent</item>
        <item name="android:fullDark">@color/m_transparent</item>
        <item name="android:topBright">@color/m_transparent</item>
        <item name="android:topDark">@color/m_transparent</item>
    </style>
</resources>